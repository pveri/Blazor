@page "/counter"
@inject IJSRuntime _js


<h1>Counter</h1>

    <p>Current count: @currentCount</p>
   
    <button class="btn btn-primary" @onclick="IncrementCount">Click me</button>
    <button class="btn btn-primary" @onclick="JsFunc">Click me JS</button>
    <input type="number" @bind="currentCount" />

@code{
  

    private async Task JsFunc()
    {
        await _js.InvokeVoidAsync("dotnetInstanceInvocation", DotNetObjectReference.Create(this));
    }
}